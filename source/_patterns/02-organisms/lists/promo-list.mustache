<ul class="o-layout o-layout--overflow {{ styleModifier }}">

    {{# promoList }}
    <li class="o-layout__item u-min-220 u-1/5">
        {{> molecules-media-promo }}
    </li>
    {{/ promoList }}

    <div class="o-layout__overflow-nav o-layout__overflow-nav--left">
        <button class="c-btn c-btn--icon c-btn--secondary c-btn--left c-btn--wall">
            <span class="u-ic-keyboard-arrow-left"></span>
        </button>
    </div>

    <div class="o-layout__overflow-nav o-layout__overflow-nav--right">
        <button class="c-btn c-btn--icon c-btn--secondary c-btn--right c-btn--wall">
            <span class="u-ic-keyboard-arrow-right"></span>
        </button>
    </div>

</ul><!--end tout-list-->
